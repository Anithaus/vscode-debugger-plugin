{
  "name": "debugger",
  "displayName": "temporal-debugger-plugin",
  "description": "Temporal VSCode Debugger plugin",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.68.0"
  },
  "activationEvents": [
    "onCommand:temporal-debugger-plugin.start",
    "onCommand:temporal-debugger-plugin.refresh"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "temporal-debugger-plugin.start",
        "title": "Launch Temporal Debugger Plugin"
      },
      {
        "command": "temporal-debugger-plugin.refresh",
        "title": "Refresh"
      }
    ]
  },
  "categories": [
    "Debuggers",
    "Visualization"
  ],
  "keywords": [
    "temporal",
    "workflow",
    "debugger",
    "vscode"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@tsconfig/node16": "^1.0.3",
    "@tsconfig/svelte": "^3.0.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.0.0",
    "@types/node-fetch": "^2.6.2",
    "@types/polka": "^0.5.4",
    "@types/vscode": "^1.68.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vscode/test-electron": "^2.1.5",
    "concurrently": "^7.3.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-tsdoc": "^0.2.16",
    "glob": "^8.0.3",
    "prettier": "2.7.1",
    "rollup": "^2.77.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "concurrently \"rollup -c -w\" \"webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "format": "prettier --write .",
    "lint": "eslint src --ext .ts --no-error-on-unmatched-pattern && prettier --end-of-line auto --check .",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run test-compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "vscode-uri": "^3.0.3"
  }
}
